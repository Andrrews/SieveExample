{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlLite": "Data Source=app.db"
  },

  "Sieve": {
    "CaseSensitive": "false",
    "DefaultPageSize": "25",
    "MaxPageSize": "50",
    "ThrowExceptions": "false",
    "IgnoreNullsOnNotEqual": "true",
    "DisableNullableTypeExpressionForSorting": "false"
  }
}
